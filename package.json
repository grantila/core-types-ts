{
  "name": "core-types-ts",
  "version": "0.0.0-development",
  "description": "core-types ⬌ TypeScript interface conversion",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/core-types-ts/issues"
  },
  "homepage": "https://github.com/grantila/core-types-ts#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {},
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=14.13.1 || >=16.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.prod.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "cz": "git-cz --disable-emoji",
	"format": "prettier --write --no-editorconfig lib/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/core-types-ts"
  },
  "keywords": [
    "type",
    "types",
    "generic",
    "typescript",
    "json"
  ],
  "dependencies": {
    "core-types": "^3.1.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^4.4.1",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "resolver": "ts-jest-resolver",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "<rootDir>/lib/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
